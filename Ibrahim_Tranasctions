"Withstanding
SELECT 
    COUNT(s.ShipmentID) AS OutstandingShipments
FROM Shipping s
JOIN Company dist ON s.DistributorID = dist.CompanyID
WHERE s.PromisedDate BETWEEN ? AND ?  -- Replace ? with start and end dates
  AND (s.ActualDate IS NULL OR s.ActualDate > ?)  -- Replace ? with end date
  AND dist.CompanyName = ?; 
"Delivered"
SELECT 
    COUNT(s.ShipmentID) AS DeliveredShipments
FROM Shipping s
JOIN Company dist ON s.DistributorID = dist.CompanyID
WHERE s.PromisedDate BETWEEN ? AND ?  -- Replace ? with start and end dates
  AND s.ActualDate IS NOT NULL 
  AND s.ActualDate <= ?  -- Replace ? with end date
  AND dist.CompanyName = ?;
